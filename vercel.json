{
    "version": 2,
    "builds": [
        {
            "src": "VaibhavPortfolioV2.html",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/VaibhavPortfolioV2.html"
        },
        {
            "source": "/(.*)",
            "destination": "/VaibhavPortfolioV2.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        }
    ]
}


